{
  "name": "saucedemo",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "test": "cypress run",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome --headless",
    "test:fast": "cypress run --headless --reporter min",
    "test:login": "cypress run --spec 'cypress/e2e/login.feature' --headless",
    "test:inventory": "cypress run --spec 'cypress/e2e/inventory.feature' --headless",
    "test:cart": "cypress run --spec 'cypress/e2e/cart.feature' --headless",
    "test:sql-demo": "cypress run --spec 'cypress/e2e/inventory.feature' --headless",
    "test:parallel": "cypress run --record --parallel",
    "open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/**/*.js",
    "json": {
      "enabled": true,
      "output": "cypress/reports/cucumber-report.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/reports/cucumber-report.html"
    }
  },
  "devDependencies": {
    "cypress": "^14.5.2",
    "cypress-mochawesome-reporter": "^4.0.0"
  }
}
