{
  "name": "saucedemo",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress run --headed",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "open": "cypress open",
    "test:api": "cypress run --spec 'cypress/e2e/**/*api*.feature'",
    "test:ui": "cypress run --spec 'cypress/e2e/**/!()*api*.feature'",
    "test:api-auth": "cypress run --spec 'cypress/e2e/api-auth/**/*.feature'",
    "test:api-auth:headed": "cypress run --spec 'cypress/e2e/api-auth/**/*.feature' --headed",
    "test:performance": "cypress run --spec 'cypress/e2e/api-auth/**/*.feature' --browser chrome --headed",
    "report": "start cypress/reports/cucumber-report.html"
  },
  "private": true,
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/**/*.js",
    "json": {
      "enabled": true,
      "output": "cypress/reports/cucumber-report.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/reports/cucumber-report.html"
    }
  },
  "devDependencies": {
    "cypress": "^14.5.2",
    "cypress-mochawesome-reporter": "^4.0.0"
  }
}
